	</styles>

	<DOTABPInternational2020EventGamePage class="DashboardPage BattlePassPage" play-button="disable">
		<Panel id="CompendiumBody" class="CompendiumBody">
            <Panel class="GateGlow"/>
            <Panel id="MainContents">

                <Panel class="SummerEventHeader">
                    <Label id="Overline" text="#DOTA_TI10_EventGame_Overline" />
                    <Label id="Title" text="#DOTA_TI10_EventGame_Page_Title" />
                    <Label id="IntroText" text="#DOTA_TI10_SummerEvent_Page_Desc" />
                </Panel>
                <Panel class="EventGameTeasers">
                    <Panel class="EventGameTeaser">
                        <Panel id="Teaser1" class="EventGameThumbnail"/>
                        <Label class="EventGameThumbnailTitle" text="#DOTA_TI10_EventGameTeaser1_Title" />
                        <Label class="EventGameThumbnailDesc" text="#DOTA_TI10_EventGameTeaser1_Desc" />
                    </Panel>
                    <Panel class="EventGameTeaser">
                        <Panel id="Teaser2" class="EventGameThumbnail"/>
                        <Label class="EventGameThumbnailTitle" text="#DOTA_TI10_EventGameTeaser2_Title" />
                        <Label class="EventGameThumbnailDesc" text="#DOTA_TI10_EventGameTeaser2_Desc" />
                    </Panel>
                    <Panel class="EventGameTeaser">
                        <Panel id="Teaser3" class="EventGameThumbnail"/>
                        <Label class="EventGameThumbnailTitle" text="#DOTA_TI10_EventGameTeaser3_Title" />
                        <Label class="EventGameThumbnailDesc" text="#DOTA_TI10_EventGameTeaser3_Desc" />
                    </Panel>
                </Panel>
            </Panel>
            <Panel id="PlayGame" class="LeftRightFlow">


            <Panel id="LowerContent">
                <Panel id="Points" class="TopBottomFlow">


                    <Panel id="PointsSection">
                        <Panel id="PointsEarnedSection" class="LeftRightFlow">
                            <Label id="BattlePointsEarnedDesc" text="#DOTA_TI10_EventGame_BP_Earned"/>
                            <Label id="BattlePointsEarnedValue" text="#DOTA_TI10_EventGame_BP_EarnedValue"/>

                        </Panel>
                        <Panel id="ProgressBarContainer" class="LeftRightFlow">

                            <ProgressBar id="PointsProgressBar" value="0"/>
                            <Panel id="InfoIcon" class="InfoIcon" onmouseover="DOTAShowSummerEventBPTooltip()" onmouseout="DOTAHideSummerEventBPTooltip()" />
                        </Panel>
                        <Label id="ResetTimer" text="DOTA_TI10_EventGame_RefreshTime" />
                    </Panel>
                </Panel>

              
                <Panel id="PlaySection">
                    <Panel id="StartMatchContainer" class="TopBottomFlow">
                        <TextButton id="StartMatchButton" class="PlayButton" text="#DOTA_TI10_EventGame_PlayButton" onactivate="DOTAStartFindingInternational2020EventGameMatch()"/>
                        <Label id="FindingMatchLabel" text="Finding Match..."/>
                    </Panel>

                    <Panel id="DailyBonusUsed" class="TopBottomFlow">
                        <Label id="BattlePointsDailyBonusTokens" text="#DOTA_TI10_EventGame_BP_AdjustPointsRemaining"/>
                        <Countdown id="DailyBonusCountdown">
                            <Label text="#DOTA_TI10_EventGame_NextDailyBonus" />
                        </Countdown>
                    </Panel>

                    <Panel id="DailyBonusActive" class="TopBottomFlow">
                        <Label id="BattlePointsDailyBonusTokens" text="#DOTA_TI10_EventGame_BP_AdjustPointsActive"/>
                    </Panel>

                </Panel>

            </Panel>

            <!--<Panel id="CampaignVignette">
                
                <Panel id="LeftColumn">
                </Panel>

                <Panel id="CenterColumn" class="CenterColumn CenterAlign TopBottomFlow">

                    <Label id="EventGameTitle" text="#DOTA_TI10_EventGame_Title" class="EventGameTitle" html="true"/>

                    <Label id="EventGameDescription" text="#DOTA_TI10_EventGame_Description" class="EventGameDescription" html="true"/>

                    <Panel id="Filler" class="FillHeight"/>

