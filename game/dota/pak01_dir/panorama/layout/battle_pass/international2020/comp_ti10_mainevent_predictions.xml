<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/dashboard_page_shared.vcss_c" />
		<include src="s2r://panorama/styles/battle_pass/international2020/bp_international2020_shared.vcss_c" />
		<include src="s2r://panorama/styles/battle_pass/international2020/comp_ti10_mainevent_predictions.vcss_c" />
	</styles>

	<script>
	</script>

	<DOTACompTI10MainEventPredictionsPage class="DashboardPage BattlePassPage" hittest="false" hidebackground="true">
		<Panel id="CompendiumBody" class="PredictionsBody ">
			<Panel id="PredictionsCentered">
				<Panel class="Header">
					<Panel class="TI10QualifierTitleBlock">
						<Image class="TI10InternationalLogo" />
						<Label class="TI10QualifierTitle" text="#DOTA_Compendium_RegionalQualifiers" />
					</Panel>
					<Panel class="HeaderGroup QualifierGroup">
						<Label class="HeaderGroupLabel" text="#DOTA_Compendium_SecondaryTabHeader_RegionalQualifiers" />
					</Panel>
					<Panel class="HeaderGroup MainEventGroup">
						<Label class="HeaderGroupLabel" text="#DOTA_CompendiumRecap_MainEvent" />
					</Panel>
					<Panel class="LockTime TimeRemaining">
						<Label class="LockTimeText StartIn" text="#DOTA_Compendium_PredictionsStartIn"/>
						<Label class="LockTimeText LockIn" text="#DOTA_Compendium_PredictionsLockIn"/>
						<Label class="Countdown MonoNumbersFont" text="#DOTA_Compendium_Predictions_ResetTime"/>
					</Panel>
					<Panel class="LockTime Timeout">
						<Label class="LockTimeText" text="#DOTA_Compendium_PredictionsLocked"/>
					</Panel>
					<Panel class="LockTime QualifierTimeRemaining">
						<Label class="LockTimeText StartIn" text="#DOTA_Compendium_PredictionsStartIn"/>
						<Label class="LockTimeText LockIn" text="#DOTA_Compendium_PredictionsLockIn"/>
						<Label class="Countdown MonoNumbersFont" text="#DOTA_Compendium_Predictions_QualLockTime"/>
					</Panel>
					<Panel class="LockTime QualifierTimeout">
						<Label class="LockTimeText" text="#DOTA_Compendium_PredictionsLocked"/>
					</Panel>
				</Panel>
				<Panel class="Tables MainEventGroup">
					<Panel id="HeaderNav">
						<TabButton id="HeroTab" class="BattlePassSecondaryTabButton" group="PredictionGroup" text="#DOTA_Compendium_Heroes" selected="true" onactivate="AddStyle('PredictionsCentered');RemoveStyle('PredictionsCentered')" />
						<Label class="TabSeperator" text="/"/>
						<TabButton id="TeamTab" class="BattlePassSecondaryTabButton" group="PredictionGroup" text="#DOTA_Compendium_Teams" onactivate="AddStyle('PredictionsCentered');RemoveStyle('PredictionsCentered')" />
						<Label class="TabSeperator" text="/"/>
						<TabButton id="PlayerTab" class="BattlePassSecondaryTabButton" group="PredictionGroup" text="#DOTA_Compendium_Players" onactivate="AddStyle('PredictionsCentered');RemoveStyle('PredictionsCentered')" />
						<Label class="TabSeperator" text="/"/>
						<TabButton id="MiscTab" class="BattlePassSecondaryTabButton" group="PredictionGroup" text="#DOTA_Compendium_Header_Tournament" onactivate="AddStyle('PredictionsCentered');RemoveStyle('PredictionsCentered')" />
					</Panel>
				</Panel>
					
				<Panel class="Predictions">
					<Label class="QualifierPredictionRewardNote" text="#DOTA_Compendium_QualiferPredictionsRewardNote" html="true" />
					<Panel id="QualifierPredictionContainer" class="QualifierGroup">
						<!-- Filled in by code -->
					</Panel>
					<TabContents tabid="HeroTab" group="PredictionGroup" class="PredictionSubTab MainEventGroup" >
						<Panel id="HeroPredictionContainer">
							<!-- Filled in by code -->
						</Panel>
					</TabContents>
					<TabContents tabid="TeamTab" group="PredictionGroup" class="PredictionSubTab MainEventGroup" >
						<Panel id="TeamPredictionContainer">
							<!-- Filled in by code -->
						</Panel>					
					</TabContents>
					<TabContents tabid="PlayerTab" group="PredictionGroup" class="PredictionSubTab MainEventGroup" >
						<Panel id="PlayerPredictionContainer">
							<!-- Filled in by code -->
						</Panel>					
					</TabContents>
					<TabContents tabid="MiscTab" group="PredictionGroup" class="PredictionSubTab MainEventGroup" >
						<Panel id="MiscPredictionContainer">
							<!-- Filled in by code -->
						</Panel>						
					</TabContents>
				</Panel>
						
			</Panel>
		</Panel>
	</DOTACompTI10MainEventPredictionsPage>
</root>
