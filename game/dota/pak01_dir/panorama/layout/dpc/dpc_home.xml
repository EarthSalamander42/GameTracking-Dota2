<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/dashboard_page_shared.vcss_c" />
		<include src="s2r://panorama/styles/dpc/dpc_home.vcss_c" />
	</styles>

	<DotaWatchDPCPage class="WatchDPC DashboardPage MajorLive">
		<DotaDPCScheduleWidget class="WatchDPCSchedule" />
		<Panel id="widgets" class="WatchDPC_Widgets">
			<Panel class="SupportTeams" onactivate="DOTAShowWatchDPCFanContentPage()">
				<Panel class="NewBadge" >
					<Label id="NewTeams" text="#DOTA_TeamFanContent_New" />
					<Panel id="NewTeamsEdge" />
				</Panel>
				<Panel class="SupportMessaging">
					<Label class="SupportTitle" text="#DOTA_DPCFanContent_SupportersClub"/>
					<Label class="SupportSubTitle" text="#DOTA_DPCFanContent_TeamBadges"/>
					<Label class="SupportAction" text="#DOTA_DPCFanContent_ViewTeams"/>
				</Panel> 
			</Panel>

			<Panel class="TI10SupportTeams" onactivate="DOTAShowWatchDPCFanContentPage()">
				<Panel class="NewBadge" >
					<Label id="NewTeams" text="#DOTA_TeamFanContent_New" />
					<Panel id="NewTeamsEdge" />
				</Panel>
				<Panel class="SupportMessaging">
					<Label class="SupportTitle" text="#DOTA_DPCFanContent_SupportersClub"/>
					<Label class="SupportSubTitle" text="#DOTA_DPCFanContent_Teams50Off"/>
					<!--                                                                   -->
				</Panel> 
			</Panel>

			<DotaDPCTeamWidget class="WatchDPCTeams" />
			<Panel class="MajorInfo">
				<Panel class="MajorLogo"/>
				<Panel class="MajorDetails">
					<Label class="MajorTitle" text="#DOTA_DPC_Kyiv2021_Season_2"/>
					<Label class="MajorName" text="#DOTA_DPC_Kyiv2021"/>
					<Label class="MajorDates" text="#DOTA_DPC_Kyiv2021Dates"/>
					<Label class="MajorPrizePoolTitle" text="#DOTA_DPC_PrizePool"/>
					<Label class="MajorPrizePool" text="#DOTA_DPC_Kyiv2021PrizePool"/>
					<Label class="MajorTeams" text="#DOTA_DPC_Teams"/>
					<Label class="MajorTeamCount" text="#DOTA_DPC_Kyiv2021Teams"/>
				</Panel>
			</Panel>
			<DotaDPCMiniScheduleWidget class="MajorUpcomingMatches"/>
			<DotaDPCStandingsWidget class="WatchDPCStandings Condensed" selected-stage="100" />
			<Panel class="TI10UpperBlock">
				<Panel class="TI10GroupStagePanel">
					<Label class="InfoSubText" text="#DOTA_Compendium_GroupStage"/>
					<Label class="InfoMainText" html="true" text="#DOTA_Compendium_GroupStageDates_Short"/>
				</Panel>
				<Panel class="TI10UpperBlockLogo" />
				<Panel class="TI10MainEventPanel">
					<Label class="InfoSubText" text="#DOTA_Compendium_MainEvent"/>
					<Label class="InfoMainText" html="true" text="#DOTA_Compendium_MainEventDates_Short"/>
				</Panel>
			</Panel>
			<Panel class="TI10LowerBlock">
				<Label class="TI10TeamsHeader" text="#DOTA_Compendium_TheTeams"/>
				<Panel class="TeamsRow LeftRightFlow">
					<DOTATeamImage id="Team1Logo" class="TeamLogo" teamid="39" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(39, 1, 1)"/>
					<DOTATeamImage id="Team2Logo" class="TeamLogo" teamid="15" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(15, 5, 1)"/>
					<DOTATeamImage id="Team3Logo" class="TeamLogo" teamid="1883502" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(1883502, 4, 1)"/>
					<DOTATeamImage id="Team4Logo" class="TeamLogo" teamid="7390454" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(7390454, 1, 1)"/>
