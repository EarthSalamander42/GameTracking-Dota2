		<snippet name="BattlePassTip">
			<Panel class="BattlePassTip">
				<DOTAAvatarImage id="Avatar" />
				<Panel class="TipCountContainer">
					<Panel class="TipToken First" />
					<Panel class="TipToken Second" />
					<Panel class="TipToken Third" />
