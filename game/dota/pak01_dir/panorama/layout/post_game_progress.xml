					<Label class="MatchRewardsTableRowLabel" text="{s:xp_type}" />
					<Panel class="MatchRewardsChallengeDetailsBottom">
						<Panel class="MatchRewardsChallengeStars">
							<Panel class="MatchRewardsChallengeStar Star1" />
							<Panel class="MatchRewardsChallengeStar Star2" />
							<Panel class="MatchRewardsChallengeStar Star3" />
						</Panel>
						<Label class="HeroBadgeProgressRowChallengeName" text="{s:challenge_name}" html="true" />
