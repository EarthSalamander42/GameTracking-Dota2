		<snippet name="MatchRewardsShardRow">
			<Panel class="MatchRewardsTableRow">
				<Label class="MatchRewardsTableRowLabel" text="{s:reward_type}" />
				<Label class="MatchRewardsTableRowValue" text="#DOTA_PlusPostGame_ShardValue" />
				<Panel class="DotaPlusCurrencyIcon" />
			</Panel>
		</snippet>
		
		<snippet name="MatchRewardsGuildContract">
			<Panel class="MatchRewardsGuildContract">
				<DOTAGuildContract id="GuildContract" />
				<Panel class="ContractCompletedIcon" />
			</Panel>
		</snippet>
		
		<snippet name="MatchRewardsGuildPointsRow">
			<Panel class="MatchRewardsTableRow">
				<Label class="MatchRewardsTableRowLabel" text="{s:reward_type}" />
				<Label class="MatchRewardsTableRowValue" text="#DOTA_GuildPointsIncrease" />
