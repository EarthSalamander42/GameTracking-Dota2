<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/post_game_progress.vcss_c" />
	</styles>

	<scripts>
		<include src="s2r://panorama/scripts/util.vjs_c" />
		<include src="s2r://panorama/scripts/sequence_actions.vjs_c" />
		<include src="s2r://panorama/scripts/mvp_accolade_type_map.vjs_c" />
		<include src="s2r://panorama/scripts/post_game_mvp.vjs_c" />
		<include src="s2r://panorama/scripts/post_game_progress/post_game_progress_utils.vjs_c" />
		<include src="s2r://panorama/scripts/post_game_progress/post_game_progress_screens.vjs_c" />
		<include src="s2r://panorama/scripts/post_game_progress/post_game_progress_coach_rating.vjs_c" />
		<include src="s2r://panorama/scripts/post_game_progress/post_game_progress_hero_badge.vjs_c" />
		<include src="s2r://panorama/scripts/post_game_progress/post_game_progress_hero_relics.vjs_c" />
		<include src="s2r://panorama/scripts/post_game_progress/post_game_progress_match_rewards.vjs_c" />
		<include src="s2r://panorama/scripts/post_game_progress/post_game_progress_sequence.vjs_c" />
	</scripts>

	<snippets>

		<!-- MVP 2.0 -->
		<snippet name="HonorableMention">
			<Panel class="HonorableMention">
				<Panel class="HeroCenterContent" hittest="false">
					<Panel id="HeroBackground" hittest="false">
						<Panel class="HeroBackgroundImage"/>
						<Panel class="HeroSelectGlow"/>
						<Panel class="Accolades" id="Accolades"/>
						<DOTAScenePanel id="HonorableMentionModel" class="OverviewHeroRender" unit="npc_dota_hero_rubick" drawbackground="0"  antialias="true" activity-modifier="PostGameIdle" particleonly="false" />
						<Panel class="HeroImageOverlay" hittest="false"/>
					</Panel>
					<Label id="PlayerName"	class="OverviewHeroPlayerName" hittest="false" html="true" text="{g:dota_persona:ng:user_account_id}"/>
					<Label id="BotPlayerName" class="OverviewHeroPlayerName" hittest="false" text="{s:bot_player_name}"/>
					<Label class="Guildname" hittest="false" html="true" text="{g:dota_guild_name:user_guild_id}"/>
					<DOTAHeroImage id="MainHeroImage" heroimagestyle="landscape" scaling="stretch-to-fit-preserve-aspect" />
				</Panel>
			</Panel>
		</snippet>

		<snippet name="MVPAccolade">
			<Panel class="Accolade MVPAccolade">
				<Panel class="gradient" id="gradient"/>
				<Panel class="icon" id="icon"/>
				<DOTAAbilityImage class="ability_icon" id="ability_icon"/>
				<Label class="title" text="{s:title}"/>
				<Label class="details" id="details" text="{s:details}" html="true"/>
			</Panel>
		</snippet>

		<!-- MVP Voting Progress -->

		<snippet name="MVPVotingProgress">
			<Panel class="ProgressScreen" >
				<DOTAPostGameMVPVote id="PostGameMVPVote" />
			</Panel>
		</snippet>

		<!-- Gauntlet Mode Progress -->
		<snippet name="GauntletProgress">
			<Panel class="ProgressScreen" onactivate="StartSkippingAhead()">
				<Panel class="BattlePassBackground">
					<Panel class="BattlePassShieldImage" />
					<Panel class="BattlePassBackgroundImage" />
				</Panel>

				<Panel class="GauntletProgressMain">
					
					<Label class="GauntletProgressTitle" text="#DOTA_Gauntlet_PlayLabel" />
					
					<Panel id="GauntletTier">
								<Label class="GauntletLabel" text="#DOTA_Gauntlet_GauntletTierTopLabel" />
								<Label class="GauntletBigLabel GauntletTier" text="#DOTA_Gauntlet_GauntletTierLabel" />
					</Panel>
					<Panel id="GauntletHeroArt">
						<!-- <Panel id="GauntletHeroesLeft" />
						<Panel id="GauntletHeroesRight" /> -->
					</Panel>
					
					<Panel class="GauntletProgressSpacer" />
					<Panel class="GauntletPipContainer">
						<Panel id="GauntletWin0" class="GauntletPip GauntletWin" />
						<Panel id="GauntletWin1" class="GauntletPip GauntletWin" />
						<Panel id="GauntletWin2" class="GauntletPip GauntletWin" />
						<Panel class="GauntletPipSpacer" />
						<Panel id="GauntletLose0" class="GauntletPip GauntletLose" />
						<Panel id="GauntletLose1" class="GauntletPip GauntletLose" />
					</Panel>
					<Panel class="GauntletComplete">
						<Panel class="GauntletInfo GauntletInfo__Win">
							<Label class="GauntletInfoDesc" text="#DOTA_GauntletPostGame_Win" />
						</Panel>
						<Panel class="GauntletInfo GauntletInfo__Promote">
							<Label class="GauntletInfoDesc" text="#DOTA_GauntletPostGame_Promote" />
						</Panel>
						<Panel class="GauntletInfo GauntletInfo__Lose">
							<Label class="GauntletInfoDesc" text="#DOTA_GauntletPostGame_Lose" />
						</Panel>
						<Panel class="GauntletInfo GauntletInfo__InProgress">
							<Label class="GauntletInfoDesc" text="#DOTA_GauntletPostGame_InProgress" />
						</Panel>
						<Panel class="GauntletInfo GauntletInfo__Rewards">
							<Panel class="GauntletInfoRewards GauntletInfoRewards__SideShop">
								<Label class="GauntletRewardsLabel" text="+{d:gauntlet_gold}"/>
								<Panel class="GauntletRewardsIcon" />
							</Panel>
							<Panel class="GauntletInfoRewards GauntletInfoRewards__PlusShards">
								<Label class="GauntletRewardsLabel" text="+{d:gauntlet_shards}" />
								<Panel class="GauntletRewardsIcon DotaPlusCurrencyIcon" />
							</Panel>
						</Panel>
					</Panel>
				</Panel>

				<Panel class="GauntletBattlePassProgress">
					<Panel id="BattlePassProgressCenter">
						<Panel class="BattlePassProgressBarTop">
							<DOTAEventLevelShield id="BattlePassLevelShield" />
							<Panel class="BattlePassPointsTotalContainer">
								<!-- <Label class="BattlePassPointsTotalTitle" text="#DOTA_BattlePassPostGame_Total" /> -->
								<Panel class="BattlePassPointsTotalGlowContainer">
									<Label class="BattlePassPointsTotal" text="+{d:total_points_gained}" />
								</Panel>
							</Panel>
						</Panel>

						<Panel id="BattleLevelProgressContainer">
							<!-- <Panel class="BattleShieldSpacer" /> -->
							<Panel class="ProgressBarContainer" >
								<ProgressBarWithMiddle id="BattleLevelProgress" min="0" max="1000" />
								<Panel id="BattleLevelProgressSection">
									<Label class="BattlePassProgressLevelProgressTitle" text="#DOTA_BattlePassPostGame_LevelProgress" />
									<Label class="BattlePassLevelProgressBP" text="#DOTA_BattlePass_BPProgress" />
								</Panel>
							</Panel>
						</Panel>
					</Panel>
				</Panel>

				<!-- should be last to stay on top -->
				<Panel id="GauntletAnimatePip" class="GauntletAnimatePip" />
			</Panel>
		</snippet>
		
		<!-- Hero Badge Progress Screen -->

		<snippet name="HeroBadgeProgress">
			<Panel class="ProgressScreen" onactivate="StartSkippingAhead()">

				<Panel class="PlusBackground" />

				<Panel id="HeroBadgeProgressMain">
					<DOTAScenePanel id="HeroBadgeHeroModel" particleonly="false" antialias="true" />

					<Panel id="HeroBadgeProgressCenter">
						<Panel class="HeroProgressTitle">
							<Panel class="HeroProgressPlusIcon" />
							<Label text="#DOTA_PlusPostGame_HeroProgress" />
						</Panel>

						<Panel id="HeroBadgeLevelProgressSection">
							<Label class="HeroBadgeProgressLevelProgressTitle" text="#DOTA_PlusPostGame_LevelProgress" />
							<Label class="HeroBadgeLevelProgressXP" text="#DOTA_Plus_XPProgress" />
						</Panel>

						<Panel id="HeroBadgeLevelProgressContainer">
							<ProgressBar id="HeroBadgeLevelProgress" min="0" max="1000" />
						</Panel>

						<Label class="HeroBadgeProgressCurrentLevel" text="#DOTA_PlusPostGame_CurrentLevel" />

						<Panel id="HeroBadgeProgressCore">
							<Panel id="HeroBadgeProgressXPGrid">
								<Panel id="HeroBadgeProgressRows">

								</Panel>
								<Panel id="TotalsRow" class="HeroBadgeProgressRow">
									<Label class="HeroBadgeProgressRowType" text="#DOTA_PlusPostGame_Total" />
									<Label class="HeroBadgeProgressRowValue" text="#DOTA_PlusPostGame_XPValue" />
								</Panel>
							</Panel>
						</Panel>
					</Panel>

					<Panel id="HeroRelicsProgressCenter">
						<Panel class="HeroProgressTitle">
							<Panel class="HeroProgressPlusIcon" />
							<Label text="#DOTA_PlusPostGame_RelicsProgress" />
						</Panel>
						<Panel id="HeroRelicsProgressList">
						</Panel>
					</Panel>

					<DOTAHeroBadge id="HeroBadgeProgressHeroBadge" badgestyle="icon" />
				</Panel>
				<Panel id="HeroBadgeLevelUpPanel" >
					<DOTAScenePanel id="LevelUpRankScene" map="scenes/hero_level_icon" camera="shot_camera" renderdeferred="true" deferredalpha="true" antialias="true" hittest="false" particleonly="false" />
					<Label id="LevelUpTitle" text="#DOTA_PlusPostGame_LevelUp" />
					<Label id="LevelUpValue" text="#DOTA_PlusPostGame_CurrentLevel" />

					<Panel id="HeroBadgeProgressRewards">
						<Panel id="HeroBadgeProgressRewardsList" />
						<TextButton id="RewardsFinishedButton" class="PopupButton" text="#DOTA_Continue" />
					</Panel>
				</Panel>
			</Panel>
		</snippet>

		<snippet name="HeroBadgeProgressRow">
			<Panel class="HeroBadgeProgressRow">
				<Label class="HeroBadgeProgressRowType" text="{s:xp_type}" />
				<Label class="HeroBadgeProgressRowValue" text="#DOTA_PlusPostGame_XPValue" />
			</Panel>
		</snippet>

		<snippet name="HeroBadgeProgressRow_Challenge">
			<Panel class="HeroBadgeProgressRow HeroBadgeProgressRow_Challenge">
				<Panel class="HeroBadgeProgressRowChallengeDetails">
					<Label class="HeroBadgeProgressRowType" text="{s:xp_type}" />
					<Panel class="HeroBadgeProgressRowChallengeDetailsBottom">
						<Panel class="HeroBadgeProgressRowChallengeStars">
							<Panel class="HeroBadgeProgressRowChallengeStar Star1" />
							<Panel class="HeroBadgeProgressRowChallengeStar Star2" />
							<Panel class="HeroBadgeProgressRowChallengeStar Star3" />
						</Panel>
						<Label class="HeroBadgeProgressRowChallengeName" text="{s:challenge_name}" html="true" />
					</Panel>
				</Panel>
				<Label class="HeroBadgeProgressRowValue" text="#DOTA_PlusPostGame_XPValue" />
			</Panel>
		</snippet>

		<snippet name="HeroBadgeProgressRow_ShardReward">
			<Panel class="HeroBadgeProgressRow ShardReward">
				<Label class="HeroBadgeProgressRowType" text="{s:reward_type}" />
				<Label class="HeroBadgeProgressRowValue" text="#DOTA_PlusPostGame_ShardValue" />
				<Panel class="DotaPlusCurrencyIcon" />
			</Panel>
		</snippet>

		<snippet name="HeroBadgeLevelUpRewardTier">
			<Panel class="HeroBadgeLevelUpRewardTier HeroBadgeLevelUpReward">
				<Panel class="HeroBadgeLevelUpRewardImage PlusHeroBadgeIcon" />
				<Label class="HeroBadgeLevelUpRewardValue" text="#DOTA_PlusRewards_TierName" />
				<Panel class="HeroBadgeLevelUpRewardLine" />
				<Label class="HeroBadgeLevelUpRewardTitle" text="#DOTA_PlusRewards_HeroBadge" />
			</Panel>
		</snippet>

		<snippet name="HeroBadgeLevelUpRewardChatWheel">
			<Panel class="HeroBadgeLevelUpRewardChatWheel HeroBadgeLevelUpReward">
				<Panel id="Icon" class="HeroBadgeLevelUpRewardImage DotaPlusChatWheelIcon" acceptsfocus="true" />
				<Label class="HeroBadgeLevelUpRewardValue" text="&quot;{s:all_chat_prefix}{s:chat_wheel_message}&quot;" />
				<Panel class="HeroBadgeLevelUpRewardLine" />
				<Label class="HeroBadgeLevelUpRewardTitle" text="#DOTA_PlusRewards_HeroChatWheel" />
			</Panel>
		</snippet>

		<snippet name="HeroBadgeLevelUpRewardCurrency">
			<Panel class="HeroBadgeLevelUpRewardCurrency HeroBadgeLevelUpReward">
				<Panel class="HeroBadgeLevelUpRewardImage DotaPlusCurrencyIcon" />
				<Label class="HeroBadgeLevelUpRewardValue" text="{d:currency_amount}" />
				<Panel class="HeroBadgeLevelUpRewardLine" />
				<Label class="HeroBadgeLevelUpRewardTitle" text="#DOTA_Plus_Currency" />
			</Panel>
		</snippet>
		
		<snippet name="HeroRelicsProgress">
			<Panel class="ProgressScreen" onactivate="StartSkippingAhead()">

				<Panel class="PlusBackground" />

				<Panel id="HeroRelicsProgressMain">
					<DOTAScenePanel id="HeroRelicsHeroModel" particleonly="false" antialias="true" />

					<Panel id="HeroRelicsProgressCenter">
						<Panel class="HeroProgressTitle">
							<Panel class="HeroProgressPlusIcon" />
							<Label text="#DOTA_PlusPostGame_RelicsProgress" />
						</Panel>
						<Panel id="HeroRelicsProgressList">
						</Panel>
					</Panel>
				</Panel>
			</Panel>
		</snippet>
		
		<snippet name="SingleRelicProgress">
			<Panel class="SingleRelicProgress">
				<Panel class="SingleRelicImageContainer">
					<DOTAHeroRelicImage id="SingleRelicImage" />
				</Panel>
				<Panel class="SingleRelicProgressDetails">
					<Panel class="SingleRelicProgressValues">
						<Label class="SingleRelicCurrentProgress" text="{d:current_progress}" />
						<Label class="SingleRelicIncrementAmount" text="+{d:increment}" />
					</Panel>

					<Panel class="SingleRelicProgressLine" />
					<Label class="SingleRelicName" text="{g:dota_gem_name:relic_type}" />
				</Panel>
			</Panel>
		</snippet>
		
		<snippet name="MatchRewardsProgress">
			<Panel class="ProgressScreen" onactivate="StartSkippingAhead()">
				<Label class="MatchRewardsTitle" text="#DOTA_MatchRewards" />
				<Panel class="MatchRewardsSections">
					<Panel id="HeroLevelMatchRewardsSection" class="MatchRewardsSection">
						<Panel class="MatchRewardsSectionBackground" />
						<DOTAScenePanel id="MatchRewardsHeroLevelModel" particleonly="false" antialias="true" drawbackground="false" />
						<Panel class="MatchRewardsSectionMain">
							<Label class="MatchRewardsSectionTitle" text="{g:dota_hero_name:hero_id}" />
							<Panel class="MatchRewardsSectionControls">
								<Panel class="MatchRewardsHeroLevelProgressContainer">
									<ProgressBar id="MatchRewardsHeroLevelProgressBar" />
									<Label class="MatchRewardsHeroLevelProgressLabel" text="#DOTA_Plus_XPProgress" />
								</Panel>
								<DOTAHeroBadge id="MatchRewardsHeroBadge" />
							</Panel>
							<Panel class="MatchRewardsHeroXPTableContainer">
								<Panel class="MatchRewardsDivider" />
								<Panel id="MatchRewardsHeroXPTable" class="MatchRewardsTable">
									<!-- dynamic -->
								</Panel>
							
								<Panel class="MatchRewardsDivider" />
								<Panel id="MatchRewardsTableTotalsRow" class="MatchRewardsTableRow ShowRow ShowValue">
									<Label class="MatchRewardsTableRowLabel" text="#DOTA_MatchRewards_TotalHeroXP" />
									<Label class="MatchRewardsTableRowValue" text="#DOTA_PlusPostGame_XPValue" />
								</Panel>
							</Panel>
							<Panel class="MatchRewardsHeroLevelPlusRestrictedContainer DotaPlusContainer">
								<Panel class="MatchRewardsHeroLevelPlusRestrictedCenter">
									<Label text="#DOTA_PlusPostGame_LevelRestricted" />
									<Panel class="MatchRewardsHeroLevelPlusRestrictedImage" />
								</Panel>
							</Panel>
						</Panel>
						<Panel id="HeroBadgeLevelUpPanel" >
							<DOTAScenePanel id="LevelUpRankScene" map="scenes/hero_level_icon" camera="shot_camera" renderdeferred="true" deferredalpha="true" antialias="true" hittest="false" particleonly="false" />
							<Label id="LevelUpTitle" text="#DOTA_PlusPostGame_LevelUp" />
							<Label id="LevelUpValue" text="#DOTA_PlusPostGame_CurrentLevel" />

							<Panel id="HeroBadgeProgressRewards">
								<Panel id="HeroBadgeProgressRewardsList" />
								<TextButton id="RewardsFinishedButton" class="PopupButton" text="#DOTA_Continue" />
							</Panel>
						</Panel>

					</Panel>
					<Panel id="ShardsMatchRewardsSection" class="MatchRewardsSection">
						<Panel class="MatchRewardsSectionBackground" />
						<Panel class="MatchRewardsShardsHeader" />
						<Panel class="MatchRewardsSectionMain">
							<Label class="MatchRewardsSectionTitle" text="{d:current_shards}" />
							<Panel class="MatchRewardsSectionControls">
								<Label class="MatchRewardsShardsTitle" text="#DOTA_MatchRewards_YourShards" />
							</Panel>
							<Panel class="MatchRewardsDivider" />
							<Panel id="MatchRewardsShardsTable" class="MatchRewardsTable">
								<!-- dynamic -->
							</Panel>
							<Panel class="MatchRewardsDivider" />
							<Panel id="MatchRewardsTableTotalsRow" class="MatchRewardsTableRow ShowRow ShowValue">
								<Label class="MatchRewardsTableRowLabel" text="#DOTA_MatchRewards_TotalShards" />
								<Label class="MatchRewardsTableRowValue" text="+{d:shard_value}" />
								<Panel class="DotaPlusCurrencyIcon" />
							</Panel>
						</Panel>
					</Panel>
					<Panel id="GuildMatchRewardsSection" class="MatchRewardsSection">
						<Panel class="MatchRewardsSectionBackground" />
						<DOTAGuildImage id="MatchRewardsGuildImage" />
						<Panel class="MatchRewardsSectionMain">
							<Label class="MatchRewardsSectionTitle" text="{g:dota_guild_name:guild_id}" />
							<Panel class="MatchRewardsSectionControls" />
							<Panel class="MatchRewardsDivider TopDivider" />
							<Panel class="MatchRewardsGuildItems">
								
								<Panel id="MatchRewardsGuildContracts">
									<!-- dynamic -->
								</Panel>
								<Panel class="MatchRewardsGuildChallenge">
									<Label class="MatchRewardsGuildChallengeLabel" text="#DOTA_MatchRewards_GuildChallenge" />
									<Panel class="MatchRewardsGuildChallengeCenter">
										<Image id="MatchRewardsGuildChallengeImage" scaling="stretch-to-fit-preserve-aspect" />
										<Label class="MatchRewardsGuildChallengeDescription" text="{s:challenge_description}" html="true" />
									</Panel>
									<Panel class="MatchRewardsGuildChallengeProgressBarContainer">
										<ProgressBarWithMiddle id="MatchRewardsGuildChallengeProgressBar" />
										<Label text="{d:challenge_start_value}/{d:challenge_max_value} &lt;span class=&quot;ChallengeProgress&quot;&gt;+{d:challenge_progress}&lt;/span&gt;" html="true"/>
									</Panel>
								</Panel>
								
							</Panel>
							
							<Panel id="MatchRewardsGuildPointsTable" class="MatchRewardsTable">
								<!-- dynamic -->
							</Panel>
							
							<Panel class="MatchRewardsDivider" />

							<Panel id="MatchRewardsTableTotalsRow" class="MatchRewardsTableRow ShowRow ShowValue">
								<Label class="MatchRewardsTableRowLabel" text="#DOTA_MatchRewards_TotalGuildPoints" />
								<Label class="MatchRewardsTableRowValue" text="#DOTA_GuildPointsIncrease" />
							</Panel>	
						</Panel>
					</Panel>
				</Panel>
				<Panel id="MatchRewardsSmoke" hittest="false" />
			</Panel>
			
		</snippet>

		<snippet name="MatchRewardsHeroXPRow">
			<Panel class="MatchRewardsTableRow">
				<Label class="MatchRewardsTableRowLabel" text="{s:xp_type}" />
				<Label class="MatchRewardsTableRowValue" text="#DOTA_PlusPostGame_XPValue" />
			</Panel>
		</snippet>

