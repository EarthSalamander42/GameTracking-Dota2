<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/anti_addiction_badge.vcss_c" />
	</styles>

	<DOTAAntiAddictionBadge onmouseover="DOTAAntiAddictionShowTooltip();" onmouseout="UIHideTextTooltip();">
		<Panel class="AA_New">
			<Panel class="AA_State AA_State_Remaining">
				<Panel class="StateImage StateImage__Green" />
				<Panel class="StateImage StateImage__Yellow" />
				<Panel class="StateImage StateImage__Red" />
				<Label text="#DOTA_AntiAddiction_Label_Remaining" />
			</Panel>
		</Panel>
		<Panel class="AA_Old">
			<Panel class="AA_State AA_State__Green">
				<Panel class="StateImage StateImage__Green" />
				<Label text="#DOTA_AntiAddiction_Label_Green" />
			</Panel>
			<Panel class="AA_State AA_State__Yellow">
				<Panel class="StateImage StateImage__Yellow" />
				<Label text="#DOTA_AntiAddiction_Label_Yellow" />
			</Panel>
			<Panel class="AA_State AA_State__Red">
				<Panel class="StateImage StateImage__Red" />
				<Label text="#DOTA_AntiAddiction_Label_Red" />
			</Panel>
		</Panel>

	</DOTAAntiAddictionBadge>

</root>