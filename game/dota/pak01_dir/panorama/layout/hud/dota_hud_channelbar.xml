<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/hudstyles.vcss_c" />
		<include src="s2r://panorama/styles/hud/dota_hud_channelbar.vcss_c" />
	</styles>

	<snippets>
		<snippet name="WRArcanaChannelBar">
			<Panel class="WRArcanaCustomChannelBar">
				<DOTAParticleScenePanel class="WRArcanaChannelBarFX IsArcanaLevel1" hittest="false" particleName="particles/econ/items/windrunner/windranger_arcana/windranger_arcana_hud_channel.vpcf" particleonly="true" cameraOrigin="0 0 230" lookAt="0 0 0" fov="40"/>
				<DOTAParticleScenePanel class="WRArcanaChannelBarFX IsArcanaLevel2" hittest="false" particleName="particles/econ/items/windrunner/windranger_arcana/windranger_arcana_hud_channel_v2.vpcf" particleonly="true" cameraOrigin="0 0 230" lookAt="0 0 0" fov="40"/>
			</Panel>
		</snippet>

		<snippet name="WRArcanaChannelBar_ArcanaLevel2">
			<Panel class="WRArcanaCustomChannelBar">
				
			</Panel>
		</snippet>

	</snippets>

	<DOTAChannelBar class="Hidden" hittest="false" hittestchildren="false">
		<Panel id="CustomChannelbarContainer">
			<!-- Dynamically filled in by code -->
		</Panel>		
		<Panel id="ChannelbarContainer">
			<Panel id="ChannelBarChannel">
				<Panel id="ChannelProgressContainer">
					<Panel id="ChannelBarProgress" />
				</Panel>
				<Label id="ChannelBarLabel" text="#DOTA_Hud_ChannelBar" hittest="false" />
			</Panel>
			<Panel id="IconBorder"/>
		</Panel>
		<Panel id="ChannelBarOverlay"/>
	</DOTAChannelBar>
</root>


