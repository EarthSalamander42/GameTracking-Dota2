		<!-- Damage Types --> 
		<Panel class="GlossarySection">
			<Panel class="TopBottomFlow GlossaryCenteredHeader" >
				<Label class="GlossarySectionLabelPreHeaderCentered" text="#DOTA_Glossary_Advanced" />
				<Label class="GlossarySectionLabel" text="#DOTA_Glossary_CombatClasses" />
			</Panel>
			<Panel class="GlossarySectionContents">
				

					<Panel class="GlossarySectionContentsCenteredEnd">

						<Label class="GlossaryParagraphBullet" text="#DOTA_Glossary_CombatClasses_Desc" html="true"/>

						<Panel class="CombatClassTable">

							<Panel class="CombatClassColumnHeader">
								<Panel/>
								<Label text="#DOTA_Glossary_AttackClass_Hero" />
								<Label text="#DOTA_Glossary_AttackClass_Basic" />
								<Label text="#DOTA_Glossary_AttackClass_Pierce"/>
								<Label text="#DOTA_Glossary_AttackClass_Siege" />
							</Panel>

							<Panel class="CombatClassColumn">
								<Label class="CombatClassRowHeader" text="#DOTA_Glossary_DefenseClass_Hero" html="true"/>
								<Label text="{d:combat_class_percent_matrix_attack_0_defense_1}%" html="true"/>
								<Label text="{d:combat_class_percent_matrix_attack_1_defense_1}%" html="true"/>
								<Label text="{d:combat_class_percent_matrix_attack_2_defense_1}%" html="true"/>
								<Label text="{d:combat_class_percent_matrix_attack_3_defense_1}%" html="true"/>
							</Panel>

							<Panel class="CombatClassColumn">
								<Label class="CombatClassRowHeader" text="#DOTA_Glossary_DefenseClass_Basic" html="true"/>
								<Label text="{d:combat_class_percent_matrix_attack_0_defense_2}%" html="true"/>
								<Label text="{d:combat_class_percent_matrix_attack_1_defense_2}%" html="true"/>
								<Label text="{d:combat_class_percent_matrix_attack_2_defense_2}%" html="true"/>
								<Label text="{d:combat_class_percent_matrix_attack_3_defense_2}%" html="true"/>
							</Panel>

							<Panel class="CombatClassColumn">
								<Label class="CombatClassRowHeader" text="#DOTA_Glossary_DefenseClass_Structure" html="true"/>
								<Label text="{d:combat_class_percent_matrix_attack_0_defense_0}%" html="true"/>
								<Label text="{d:combat_class_percent_matrix_attack_1_defense_0}%" html="true"/>
								<Label text="{d:combat_class_percent_matrix_attack_2_defense_0}%" html="true"/>
								<Label text="{d:combat_class_percent_matrix_attack_3_defense_0}%" html="true"/>
							</Panel>

							<Panel class="CombatClassTableTooltip InfoIcon" onmouseout="UIHideTextTooltip();" onmouseover="UIShowTextTooltip( #DOTA_Glossary_CombatClasses_Tooltip );" />

						</Panel>
					</Panel>
				
			</Panel>
		</Panel>  <!-- End Combat Classes --> 


		<!--
		<Panel class="GlossarySection">
			<Label class="GlossarySectionLabel" text="#DOTA_Glossary_Advanced_Projectiles" />
			<Panel class="GlossarySectionContents">
				<Panel class="GlossaryRow">
				</Panel>
			</Panel>
