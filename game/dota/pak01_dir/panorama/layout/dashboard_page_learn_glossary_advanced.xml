<root>

	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/dashboard_page_shared.vcss_c" />
		<include src="s2r://panorama/styles/dashboard_page_learn_shared.vcss_c" />
		<include src="s2r://panorama/styles/hero_stat_icons.vcss_c" />
		<include src="s2r://panorama/styles/dashboard_page_learn_glossary.vcss_c" />
		<include src="s2r://panorama/styles/dashboard_page_learn_glossary_mechanics.vcss_c" />
	</styles>

	<Panel class="GlossaryPage">

		<!-- CommonBuffs --> 
		<Panel class="GlossarySection">
			<Panel class="TopBottomFlow GlossaryCenteredHeaderTop" >
				<Label class="GlossarySectionLabelPreHeaderCentered" text="#DOTA_Glossary_Advanced" />
				<Label class="GlossarySectionLabel" text="#DOTA_Glossary_Advanced_CommonBuffs" />
			</Panel>
			<Panel class="GlossarySectionContentsCentered">
				
				<Panel class="GlossarySubsection ContentsCentered">
					<Label class="GlossarySubsectionLabel" text="#DOTA_Glossary_Advanced_CommonBuffs_AttackModifiers"/>
					<Label class="GlossaryParagraphBullet" text="#DOTA_Glossary_Advanced_CommonBuffs_AttackModifiers_Desc"/>

					<Panel class="HelpEntry">
						<Label class="HelpEntryName" text="#DOTA_Glossary_Advanced_CommonBuffs_CriticalStrike" />
						<Label class="HelpEntryDescription" text="#DOTA_Glossary_Advanced_CommonBuffs_CriticalStrike_Desc"/>
					</Panel>

					<Panel class="HelpEntry">
						<Label class="HelpEntryName" text="#DOTA_Glossary_Advanced_CommonBuffs_TrueStrike" />
						<Label class="HelpEntryDescription" text="#DOTA_Glossary_Advanced_CommonBuffs_TrueStrike_Desc"/>
					</Panel>

					<Panel class="HelpEntry">
						<Label class="HelpEntryName" text="#DOTA_Glossary_Advanced_CommonBuffs_LifeSteal" />
						<Label class="HelpEntryDescription" text="#DOTA_Glossary_Advanced_CommonBuffs_LifeSteal_Desc"/>
					</Panel>

					<Panel class="HelpEntry">
						<Label class="HelpEntryName" text="#DOTA_Glossary_Advanced_CommonBuffs_Bash" />
						<Label class="HelpEntryDescription" text="#DOTA_Glossary_Advanced_CommonBuffs_Bash_Desc"/>
					</Panel>

					<Panel class="HelpEntry">
						<Label class="HelpEntryName" text="#DOTA_Glossary_Advanced_CommonBuffs_ManaBreak" />
						<Label class="HelpEntryDescription" text="#DOTA_Glossary_Advanced_CommonBuffs_ManaBreak_Desc"/>
					</Panel>

				</Panel>

				<Panel class="HelpEntry">
					<Label class="HelpEntryName" text="#DOTA_Glossary_Advanced_CommonBuffs_SpellImmunity" />
					<Label class="HelpEntryDescription" text="#DOTA_Glossary_Advanced_CommonBuffs_SpellImmunity_Desc"/>
				</Panel>

				<Panel class="HelpEntry">
					<Label class="HelpEntryName" text="#DOTA_Glossary_Advanced_CommonBuffs_Ethereal" />
					<Label class="HelpEntryDescription" text="#DOTA_Glossary_Advanced_CommonBuffs_Ethereal_Desc"/>
				</Panel>

				<Panel class="HelpEntry">
					<Label class="HelpEntryName" text="#DOTA_Glossary_Map_Vision_Invisibility" />
					<Label class="HelpEntryDescription" text="#DOTA_Glossary_Map_Vision_Invisibility_Desc"/>
				</Panel>

				<Panel class="HelpEntry">
					<Label class="HelpEntryName" text="#DOTA_Glossary_Advanced_CommonBuffs_Phased" />
					<Label class="HelpEntryDescription" text="#DOTA_Glossary_Advanced_CommonBuffs_Phased_Desc"/>
				</Panel>

				<Panel class="HelpEntry">
					<Label class="HelpEntryName" text="#DOTA_Glossary_Advanced_CommonBuffs_Auras" />
					<Label class="HelpEntryDescription" text="#DOTA_Glossary_Advanced_CommonBuffs_Auras_Desc"/>
				</Panel>

			</Panel>
		</Panel>

		<!-- Abilities --> 
		<Panel class="GlossarySection GlossarySectionDark">
			<Panel class="TopBottomFlow GlossaryCenteredHeader" >
				<Label class="GlossarySectionLabelPreHeaderCentered" text="#DOTA_Glossary_Advanced" />
				<Label class="GlossarySectionLabel" text="#DOTA_Glossary_Advanced_AbilityTargeting" />
			</Panel>
			<Panel class="GlossarySectionContentsCentered">

				<Panel class="HelpEntry">
					<Label class="HelpEntryName" text="#DOTA_Glossary_Untargeted" />
					<Label class="HelpEntryDescription" text="#DOTA_Glossary_Untargeted_Desc"/>
				</Panel>

				<Panel class="HelpEntry">
					<Label class="HelpEntryName" text="#DOTA_Glossary_UnitTargeted" />
					<Label class="HelpEntryDescription" text="#DOTA_Glossary_UnitTargeted_Desc"/>
				</Panel>

				<Panel class="HelpEntry">
					<Label class="HelpEntryName" text="#DOTA_Glossary_PointTargeted" />
					<Label class="HelpEntryDescription" text="#DOTA_Glossary_PointTargeted_Desc"/>
				</Panel>

				<Panel class="HelpEntry">
					<Label class="HelpEntryName" text="#DOTA_Glossary_AreaOfEffect" />
					<Label class="HelpEntryDescription" text="#DOTA_Glossary_AreaOfEffect_Desc"/>
				</Panel>
			</Panel>
		</Panel>  <!-- End Abilities --> 


		<!-- Unit Orders --> 
		<Panel class="GlossarySection">
			<Panel class="TopBottomFlow GlossaryCenteredHeader" >
				<Label class="GlossarySectionLabelPreHeaderCentered" text="#DOTA_Glossary_Advanced" />
				<Label class="GlossarySectionLabel" text="#DOTA_Glossary_Advanced_UnitOrders" />
			</Panel>
			<Panel class="GlossarySectionContentsCentered">
				<Panel class="GlossaryRow">

					<Panel class="HeroStatsHelpContents GlossaryLeft">

						<Panel class="HelpEntry">
							<Panel class="AbilityButton">
								<DOTAAbilityImage class="AbilityImage" src="s2r://panorama/images/spellicons/action_move_png.vtex" scaling="stretch-to-fit-preserve-aspect" />
								<Panel hittest="false" class="AbilityBevel"/>
							</Panel>
							<Label class="HelpEntryName" text="#DOTA_Glossary_Move" />
							<Label class="HelpEntryDescription" text="#DOTA_Glossary_Move_Desc"/>
						</Panel>

						<Panel class="HelpEntry">
							<Panel class="AbilityButton">
								<DOTAAbilityImage class="AbilityImage" src="s2r://panorama/images/spellicons/action_attack_png.vtex" scaling="stretch-to-fit-preserve-aspect" />
								<Panel hittest="false" class="AbilityBevel"/>
							</Panel>
							<Label class="HelpEntryName" text="#DOTA_Glossary_Attack" />
							<Label class="HelpEntryDescription" text="#DOTA_Glossary_Attack_Desc" html="true" />
						</Panel>

						<Panel class="HelpEntry">
							<Panel class="AbilityButton">
								<DOTAAbilityImage class="AbilityImage" src="s2r://panorama/images/spellicons/action_attackmove_png.vtex" scaling="stretch-to-fit-preserve-aspect" />
								<Panel hittest="false" class="AbilityBevel"/>
							</Panel>
							<Label class="HelpEntryName" text="#DOTA_Glossary_AttackMove" />
							<Label class="HelpEntryDescription" text="#DOTA_Glossary_AttackMove_Desc" html="true" />
						</Panel>

					</Panel>

					<Panel class="HeroStatsHelpContents GlossaryRight">

						<Panel class="HelpEntry">
							<Panel class="AbilityButton">
								<DOTAAbilityImage class="AbilityImage" src="s2r://panorama/images/spellicons/action_stop_png.vtex" scaling="stretch-to-fit-preserve-aspect" />
								<Panel hittest="false" class="AbilityBevel"/>
							</Panel>
							<Label class="HelpEntryName" text="#DOTA_Glossary_Stop" />
							<Label class="HelpEntryDescription" text="#DOTA_Glossary_Stop_Desc"/>
						</Panel>
						
						<Panel class="HelpEntry">
							<Panel class="AbilityButton">
								<DOTAAbilityImage class="AbilityImage" src="s2r://panorama/images/spellicons/action_hold_png.vtex" scaling="stretch-to-fit-preserve-aspect" />
								<Panel hittest="false" class="AbilityBevel"/>
							</Panel>
							<Label class="HelpEntryName" text="#DOTA_Glossary_Hold" />
							<Label class="HelpEntryDescription" text="#DOTA_Glossary_Hold_Desc"/>
						</Panel>

						<Panel class="HelpEntry">
							<Panel class="AbilityButton">
								<DOTAAbilityImage class="AbilityImage" src="s2r://panorama/images/spellicons/action_patrol_png.vtex" scaling="stretch-to-fit-preserve-aspect" />
								<Panel hittest="false" class="AbilityBevel"/>
							</Panel>
							<Label class="HelpEntryName" text="#DOTA_Glossary_Patrol" />
							<Label class="HelpEntryDescription" text="#DOTA_Glossary_Patrol_Desc"/>
						</Panel>

						<Panel class="HelpEntry">
							
							<Label class="HelpEntryName" text="#DOTA_Glossary_QueuedOrders" />

							<Panel class="TopBottomFlow HelpEntryDescription RightAlign QueuedOrdersHelpEntry">
								<Panel class="QueuedOrdersContainer LeftRightFlow">
									<Panel class="QueuedOrder NextOrder">
										<Panel class="OrderBorder"/>
										<DOTAAbilityImage class="AbilityImage QueuedOrderImage" src="s2r://panorama/images/spellicons/action_move_png.vtex" scaling="stretch-to-fit-preserve-aspect" />
									</Panel>
									<Panel class="QueuedOrder">
										<Panel class="OrderBorder"/>
										<DOTAAbilityImage class="AbilityImage QueuedOrderImage" abilityname="item_blink" />
									</Panel>
									<Panel class="QueuedOrder">
										<Panel class="OrderBorder"/>
										<DOTAAbilityImage class="AbilityImage QueuedOrderImage" abilityname="earthshaker_echo_slam" />
									</Panel>
								</Panel>
								<Label class="HelpEntryDescription" text="#DOTA_Glossary_QueuedOrders_Desc"/>
							</Panel>
						</Panel>

					</Panel>

				</Panel>
			</Panel>
		</Panel>  <!-- End Unit Orders --> 


		<Panel class="GlossarySection GlossarySectionDark">
			<Panel class="TopBottomFlow GlossaryCenteredHeader" >
				<Label class="GlossarySectionLabelPreHeaderCentered" text="#DOTA_Glossary_Advanced" />
				<Label class="GlossarySectionLabel" text="#DOTA_Glossary_Advanced_Aggro" />
			</Panel>
			<Panel class="GlossarySectionContentsCentered">
				<Panel class="GlossaryParagraphBullet"> <Label text="#DOTA_Glossary_Advanced_Aggro_Desc" /> </Panel>

				<Panel class="HelpEntry">
					<Label class="HelpEntryName" text="#DOTA_Glossary_Advanced_AcquisitionRange" html="true"/>
					<Label class="HelpEntryDescription" text="#DOTA_Glossary_Advanced_AcquisitionRange_Desc" html="true"/>
				</Panel>

				<Panel class="HelpEntry">
					<Label class="HelpEntryName" text="#DOTA_Glossary_Advanced_AttackPriority" />
					<Label class="HelpEntryDescription" text="#DOTA_Glossary_Advanced_AttackPriority_Desc"/>
				</Panel>

				<Panel class="HelpEntry">
					<Label class="HelpEntryName" text="#DOTA_Glossary_Advanced_CreepAggro" />
					<Label class="HelpEntryDescription" text="#DOTA_Glossary_Advanced_CreepAggro_Desc" html="true">
						<Panel id="CreepAggroTooltip" class="InfoIcon GlossaryFootnote" onmouseover="UIShowTextTooltipStyled( #DOTA_Glossary_Advanced_CreepAggro_Tooltip, LongTextTooltip )" onmouseout="UIHideTextTooltip()"/>
					</Label>
				</Panel>

				<Panel class="HelpEntry">
					<Label class="HelpEntryName" text="#DOTA_Glossary_Advanced_TowerAggro" />
					<Label class="HelpEntryDescription" text="#DOTA_Glossary_Advanced_TowerAggro_Desc"/>
				</Panel>

				<Panel class="HelpEntry">
					<Label class="HelpEntryName" text="#DOTA_Glossary_Advanced_NeutralCreepAggro" />
					<Label class="HelpEntryDescription" text="#DOTA_Glossary_Advanced_NeutralCreepAggro_Desc"/>
				</Panel>

			</Panel>
		</Panel>

		<!--
		<Panel class="GlossarySection">
			<Label class="GlossarySectionLabel" text="#DOTA_Glossary_Advanced_PullingStacking" />
			<Panel class="GlossarySectionContents">
				<Panel class="GlossaryRow">
				</Panel>
			</Panel>
		</Panel>

		<Panel class="GlossarySection">
			<Label class="GlossarySectionLabel" text="#DOTA_Glossary_Advanced_Economy" />
			<Panel class="GlossarySectionContents">
				<Panel class="GlossaryRow">
				</Panel>
			</Panel>
		</Panel>
